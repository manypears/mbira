{
  "name": "mbira",
  "productName": "Mbira",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@electron/remote": "^2.0.9",
    "@react-three/drei": "^9.69.2",
    "@react-three/fiber": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "bootstrap": "^5.2.3",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^2.0.0",
    "express": "^4.18.2",
    "html-webpack-plugin": "^5.5.1",
    "nedb": "^1.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-spring": "^9.7.1",
    "sass": "^1.62.1",
    "three": "^0.166.0",
    "wait-on": "^7.0.1",
    "web-vitals": "^2.1.4",
    "webpack": "^5.84.1",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^5.2.2"
  },
  "main": "public/main.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:serve": "concurrently -k \"cross-env BROWSER=none yarn start\" \"yarn electron:start\"",
    "electron:build": "yarn webpack:build && yarn build && electron-builder -c.extraMetadata.main=build/main.js",
    "electron:start": "wait-on tcp:3000 && electron .",
    "webpack:serve": "webpack serve",
    "webpack:build": "webpack"
  },
  "build": {
    "extends": null,
    "appId": "com.mbiramatepe.app",
    "productName": "Mbira",
    "files": [
      "dist/**/*",
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.22.2",
    "@babel/preset-react": "^7.22.3",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.8.1",
    "electron": "^37.2.6",
    "electron-builder": "^26.0.12",
    "electron-packager": "^17.1.1",
    "file-loader": "^6.2.0",
    "react-scripts": "^5.0.1",
    "sass-loader": "^13.3.1",
    "style-loader": "^3.3.3"
  }
}
